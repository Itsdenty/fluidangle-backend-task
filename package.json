{
  "name": "andela-vlf",
  "version": "1.0.0",
  "description": "for the andela vlf challenge",
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "main": "dist",
  "scripts": {
    "build": "NODE_ENV=development rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./public,./.babelrc,./package.json,./npm-debug.log,./test --copy-files && npm run sequelize db:migrate",
    "start": "npm run sequelize db:migrate && npm run build && node dist/index.js",
    "test": "NODE_ENV=test npm run sequelize db:migrate && nyc mocha --timeout 10000 tests/*.test.js --exit --compilers js:babel-register",
    "travis": "NODE_ENV=travis npm run sequelize db:migrate && nyc mocha --timeout 20000 tests/*.test.js --exit --compilers js:babel-register",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "keywords": [
    "andela",
    "stackoverflow-clone"
  ],
  "author": "Abd-afeez",
  "license": "ISC",
  "bugs": {
    "url": "https://***/issues"
  },
  "homepage": "https://***#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1",
    "swagger-jsdoc": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  }
}
